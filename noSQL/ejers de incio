2)
db.usuario.find(
  { edad: { $gt: 25 } }, 
  { nombre: 1, apellido: 1, _id: 0 }
)

3)
db.usuario.find({ compras: { $size: 2 } })

4)
db.usuario.find({}, {"compras.monto":1, _id: 0 })

5)
db.usuario.find({"compras.productos.5":{$size:5}}, {"compras.monto":1, _id: 0 })

6)
db.usuario.find({"compras.id_compra":1}, {"compras.$":1, _id: 0 })
7)
db.usuario.updateOne({_id: 2}, {$push: {compras: {id_compra: 20, monto: 2500, productos: ["libro","lapiz","cuaderno"]}}})
8)
db.usuario.updateOne({nombre: "Lucas", apellido: "Mart√≠nez"}, {$set: {mail: "lucas.m@gmail.com"}})
9)
db.usuario.find({},{"compras.id_compra":1,nombre:1,"compras.monto":1,_id:0}).sort({"compras.monto":-1}).limit(3)
10)
db.usuario.find({ "compras": { $not: { $elemMatch: { productos:"televisor" } } } },{ _id: 0, nombre: 1, mail: 1 }).sort({ edad: -1 });
11)
db.usuario.find({compras:{$elemMatch:{productos:"laptop"}}},{_id:1,nombre:1,apellido:1})
